    "@fortawesome/react-fontawesome": "^0.1.14",
    "@headlessui/react": "0.3.2",
    "@styled-system/css": "5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "next": "10.1.3",
    "prettier": "2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "styled-components": "5.2.3",
    "styled-system": "5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@storybook/addon-actions": "^6.2.7",
