{
  "name": "gmg-styled-system-with-nextjs",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint:ts && yarn lint:es",
    "lint:ts": "npx tsc --noEmit",
    "lint:es": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:es:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
